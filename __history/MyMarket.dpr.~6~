program MyMarket;

uses
  System.StartUpCopy,
  FMX.Forms,
  UnitLoginUsuario in 'UnitLoginUsuario.pas' {FrmLoginUsuario},
  UnitPrincipal in 'UnitPrincipal.pas' {FrmPrincipal},
  UnitDM in 'UnitDM.pas' {DataModule1: TDataModule};

